{
  "name": "joshua-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run studio:build && npm run studio:fix-paths",
    "lint": "eslint .",
    "preview": "vite preview",
    "studio": "cd studio && sanity dev",
    "studio:build": "cd studio && sanity build ../dist/studio -y",
    "studio:fix-paths": "sed -i 's|href=\"/static/|href=\"/studio/static/|g; s|src=\"/static/|src=\"/studio/static/|g' dist/studio/index.html"
  },
  "dependencies": {
    "@portabletext/react": "^3.0.18",
    "@sanity/client": "^6.15.0",
    "@sanity/image-url": "^1.0.2",
    "@sanity/vision": "^3.57.0",
    "aos": "^3.0.0-beta.6",
    "axios": "^1.7.9",
    "framer-motion": "^11.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.0.1",
    "react-lazy-load-image-component": "^1.6.0",
    "react-masonry-css": "^1.0.16",
    "react-router-dom": "^6.22.0",
    "sanity": "^3.57.0",
    "styled-components": "^6.1.8",
    "yet-another-react-lightbox": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.8.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.9",
    "vite": "^5.4.0"
  }
}
