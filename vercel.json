{
    "rewrites": [
        {
            "source": "/studio",
            "destination": "/studio/index.html"
        },
        {
            "source": "/studio/",
            "destination": "/studio/index.html"
        },
        {
            "source": "/studio/:path((?!static/).*)",
            "destination": "/studio/index.html"
        },
        {
            "source": "/((?!studio|api).*)",
            "destination": "/index.html"
        }
    ]
}
